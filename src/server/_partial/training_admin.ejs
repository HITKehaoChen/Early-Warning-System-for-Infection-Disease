<div class="col s12">

  <div class="card blue darken-3">
    <div id="admin" class="card-image">
      <div class="card-content white-text">
        <h3 class="light">训练管理</h3>
      </div>
      <div class="card-tabs">
        <ul class="tabs tabs-fixed-width tabs-transparent">
          <li class="tab"><a class="active waves-effect" href="#train">训练</a></li>
          <li class="tab"><a class="waves-effect" href="#test">测试</a></li>
          <li class="tab"><a class="waves-effect" href="#apply">应用</a></li>
          <!--<li class="tab"><a class="waves-effect" href="#fault_tolerant">容错</a></li>-->
        </ul>
      </div>
    </div>

    <div class="card-content white">
      <div id="train">
        <div class="row">

          <form id="training_train" class="col s12" method="post" enctype="multipart/form-data">

            <div class="row">

              <div class="file-field input-field col s12 m6">
                <div class="btn waves-effect">
                  <!--<span>文件</span>-->
                  <i class="material-icons">backup</i>
                  <input id="train_file1" type="file" name="train_file1">
                </div>
                <div class="file-path-wrapper">
                  <input class="file-path validate" type="text" placeholder="上传一个文件(xxx.snet)">
                </div>
              </div>

              <div class="file-field input-field col s12 m6">
                <div class="btn waves-effect">
                  <!--<span>文件</span>-->
                  <i class="material-icons">backup</i>
                  <input id="train_file2" type="file" name="train_file2">
                </div>
                <div class="file-path-wrapper">
                  <input id="train_file2" class="file-path validate" type="text" placeholder="上传一个文件(xxx.txt)">
                </div>
              </div>

              <!--<div class="col s12 right">-->
                <!--<a id="checkFile" class="btn waves-effect right yellow black-text">检查</a>-->
              <!--</div>-->
              <% const list = [
                ["sr", "输入层神经元个数"],
                ["yc", "隐藏层神经元个数"],
                ["sc", "输出层神经元个数"],
                ["xlsl", "速度"],
                ["xldl", "动量"],
                ["xlcs", "训练轮数"]
                // ["remaining", "剩余时间/轮"]
              ] %>
              <% for (let i = 0; i < list.length;i++) { %>
              <div class="input-field col s12 m6">
                <input type="number" step="any" id=<%= list[i][0] %> name=<%= list[i][0] %> class="validate">
                <label for="<%= list[i][0] %>"><%= list[i][1] %></label>
              </div>
              <% } %>
              <div class="col s12">

                <div class="right">
                  <!--<a class="btn waves-effect right yellow black-text">训练</a>-->
                  <button class="btn waves-effect yellow black-text" type="submit" name="action">训练
                    <!--<i class="material-icons right">send</i>-->
                  </button>
                </div>
                <div class="right col">
                  <a id="refresh" class="btn waves-effect yellow black-text">刷新</a>
                </div>
                <!--<button class="btn waves-effect waves-light" type="submit" name="action">Submit-->
                <!--&lt;!&ndash;<i class="material-icons right">send</i>&ndash;&gt;-->
                <!--</button>-->

              </div>
              <div class="col s12">
                <h5 id="lastCircles"></h5>
                <h5 id="globalError"></h5>
              </div>
            </div>
          </form>
        </div>
      </div>


      <div id="test">
        <div class="row">
          <form id="training_test" class="col s12" method="post" enctype="multipart/form-data">
            <div class="row">
              <div class="file-field input-field col s12 m6">
                <div class="btn waves-effect">
                  <!--<span>File</span>-->
                  <i class="material-icons">backup</i>
                  <input id="test_file1" type="file" name="train_file1">
                </div>
                <div class="file-path-wrapper">
                  <input class="file-path validate" type="text" placeholder="上传一个文件(xxx.snet)">
                </div>
              </div>

              <div class="file-field input-field col s12 m6">
                <div class="btn waves-effect">
                  <!--<span>File</span>-->
                  <i class="material-icons">backup</i>
                  <input id="test_file2" type="file" name="train_file2">

                </div>
                <div class="file-path-wrapper">
                  <input class="file-path validate" type="text" placeholder="上传一个文件(xxx.txt)">
                </div>
              </div>

              <div class="col s12 right">
                <!--<a id="testBtn"  type="submit" class="btn waves-effect right yellow black-text">Test-->
                <!--</a>-->
                <button class="btn right waves-effect yellow black-text" type="submit" name="action">Test
                </button>
              </div>


            </div>
          </form>

          <div class="col s12">
            <h5 id="accuracy"></h5>
          </div>
        </div>
      </div>


      <div id="apply">
        <div class="row">
          <form id="training_apply" class="col s12" method="post" enctype="multipart/form-data">
            <div class="row">
              <div class="file-field input-field col s12 m6">
                <div class="btn waves-effect">
                  <!--<span>File</span>-->
                  <i class="material-icons">backup</i>
                  <input id="apply_file1" type="file" name="apply_file1">
                </div>
                <div class="file-path-wrapper">
                  <input class="file-path validate" type="text" placeholder="上传一个文件(xxx.snet)">
                </div>
              </div>

              <div class="file-field input-field col s12 m6">
                <div class="btn waves-effect">
                  <!--<span>File</span>-->
                  <i class="material-icons">backup</i>
                  <input id="apply_file2" type="file" name="apply_file2">
                </div>
                <div class="file-path-wrapper">
                  <input class="file-path validate" type="text" placeholder="上传一个文件(xxx.snet)">
                </div>
              </div>


              <div class="input-field col s12 m6">
                <select name="sex">
                  <option value="1">男</option>
                  <option value="0">女</option>
                </select>
                <label>性别</label>
              </div>
              <div class="input-field col s12 m6">
                <input type="number" id="age" name="age" class="validate">
                <label for="age">年龄</label>
              </div>
              <div class="input-field col s12">
                <select name="hurttype">
                  <option value="0">典型心绞痛</option>
                  <option value="1">非典型心绞痛</option>
                  <option value="2">非心绞痛</option>
                  <option value="3">无症状</option>
                </select>
                <label>胸痛类型</label>
              </div>
              <div class="input-field col s12">
                <input type="number" id="blood_pressure" name="trestbps" class="validate">
                <label for="blood_pressure">静息血压(入院时为毫米汞柱)</label>
              </div>
              <div class="input-field col s12 m6">
                <input type="number" id="serum_cholesterol" name="chol" class="validate">
                <label for="serum_cholesterol">血清胆固醇(mg/dl)</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="fbs">
                  <option value="1">是</option>
                  <option value="0">否</option>
                </select>
                <label>空腹血糖 > 120mg/dl？</label>
              </div>

              <div class="input-field col s12">
                <select name="restecg">
                  <option value="0">正常</option>
                  <option value="1">具有ST-T波异常(T波反转和/或ST升高或抑制>0.05mV)</option>
                  <option value="2">根据Estes标准显示可能左心室肥大</option>
                </select>
                <label>静息心电图结果</label>
              </div>
              <div class="input-field col s12 m6">
                <input type="number" id="max_heart_rate" name="thalach" class="validate">
                <label for="max_heart_rate">达到的最大心率</label>
              </div>


              <div class="input-field col s12 m6">
                <select name="exang">
                  <option value="1">是</option>
                  <option value="0">否</option>
                </select>
                <label>运动引起心绞痛？</label>
              </div>
              <div class="input-field col s12">
                <input type="number" step="any" id="depression" name="oldpeak" class="validate">
                <label for="depression">运动相对于休息引起的ST下降</label>
              </div>
              <div class="input-field col s12 m6">
                <select name="slope">
                  <option value="0">上升</option>
                  <option value="1">平滑</option>
                  <option value="2">下降</option>
                </select>
                <label>峰值运动ST段的坡度</label>
              </div>
              <div class="input-field col s12 m6">
                <input type="number" id="vessels_number" name="ca" class="validate">
                <label for="vessels_number">荧光镜检查的主要血管数量(0-3)</label>
              </div>
              <div class="input-field col s12 m6">
                <select name="thal">
                  <option value="0">正常</option>
                  <option value="1">固定缺陷</option>
                  <option value="2">可逆缺陷</option>
                </select>
                <label>缺陷类型</label>
              </div>


              <div class="input-field col s12 m6">
                <select name="hxb">
                  <option value="0">正常</option>
                  <option value="1">不正常</option>
                </select>
                <label>红细胞</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="nb">
                  <option value="0">正常</option>
                  <option value="1">不正常</option>
                </select>
                <label>脓细胞</label>
              </div>


              <div class="input-field col s12 m6">
                <select name="nbtk">
                  <option value="0">存在</option>
                  <option value="1">不存在</option>
                </select>
                <label>脓细胞团块</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="xj">
                  <option value="0">存在</option>
                  <option value="1">不存在</option>
                </select>
                <label>细菌</label>
              </div>

              <div class="input-field col s12 m6">
                <input type="number" id="blood_glucose" name="xt" class="validate">
                <label for="blood_glucose">随机的血糖(mgs/dl)</label>
              </div>

              <div class="input-field col s12 m6">
                <input type="number" step="any" id="blood_urea" name="xns" class="validate">
                <label for="blood_urea">血尿素(mgs/dl)</label>
              </div>


              <div class="input-field col s12 m6">
                <input type="number" step="any" id="serum_creatinine" name="xjg" class="validate">
                <label for="serum_creatinine">血清肌酐(mgs/dl)</label>
              </div>


              <div class="input-field col s12">
                <input type="number" step="any" id="red_blood_cell_cnt" name="hxbjs" class="validate">
                <label for="red_blood_cell_cnt">红细胞计数(millions/cmm)</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="gxy">
                  <option value="1">是</option>
                  <option value="0">否</option>
                </select>
                <label>高血压</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="tnb">
                  <option value="1">是</option>
                  <option value="0">否</option>
                </select>
                <label>糖尿病</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="sy">
                  <option value="1">好</option>
                  <option value="0">差</option>
                </select>
                <label>食欲</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="zsz">
                  <option value="1">是</option>
                  <option value="0">否</option>
                </select>
                <label>足水肿</label>
              </div>

              <div class="input-field col s12 m6">
                <select name="px">
                  <option value="1">是</option>
                  <option value="0">否</option>
                </select>
                <label>贫血</label>
              </div>


              <div class="col s12">
                <button class="right btn waves-effect waves-light yellow black-text" type="submit" name="action">分析
                </button>
              </div>

            </div>
          </form>
          <!--<div class="input-field col s12">-->
          <!--<input type="number" id="analysis_res" name="analysis_res" formnovalidate>-->
          <!--<label for="analysis_res">分析结果</label>-->
          <!--</div>-->
          <div class="col s12">
            <h5 id="trainingRes"></h5>
          </div>
        </div>

      </div>

